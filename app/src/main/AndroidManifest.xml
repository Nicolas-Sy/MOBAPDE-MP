<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sparechange">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/piggy"
        android:label="@string/app_name"
        android:roundIcon="@drawable/piggy"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Register"></activity>

        <activity android:name=".Login">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>

        </activity>
        <activity
            android:name=".CategorySlider"
            android:label="@string/title_activity_category_slider"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".BudgetBreakdownActivity" />
        <activity android:name=".PerTransactionActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".addTransactionActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>